generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Raridade {
  Comum
  Raro
  MuitoRaro
  Lendario
  Ultrararo
  Desconhecido
}

model pokemons{
  id_pokemon   String    @id @default(uuid())
  nome         String
  tipo         String
  raridade     Raridade
  preco        Decimal
}